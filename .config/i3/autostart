# vim: ft=i3config

exec --no-startup-id dex --autostart --environment i3
exec_always --no-startup-id xset -dpms
exec_always --no-startup-id xset s off
exec_always --no-startup-id nitrogen --restore || feh --bg-scale ~/.config/i3/ina.png

exec --no-startup-id fcitx5
exec --no-startup-id flameshot

exec --no-startup-id warpd
exec --no-startup-id dunst

exec --no-startup-id picom --daemon

# exec --no-startup-id ~/esp/clash-verge_1.5.11_amd64.AppImage
exec --no-startup-id clash

exec_always --no-startup-id test -f ~/.Xmodmap && xmodmap ~/.Xmodmap
exec --no-startup-id test -x ~/.config/i3/autostart.private.sh && ~/.config/i3/autostart.private.sh
